<h2>Books search for 'test'</h2>
<table class="table table-striped">
	<thead>
		<tr>
			<th colspan="3"></th>
			<th>title</th>
			<th>body</th>
		</tr>
	</thead>

	<tbody>
		<% @dates.each do |date| %>
		<tr>
			<td><%= attachment_image_tag(@user, :profile_image, :fill, 50, 50, fallback: "no-image-mini.jpg") %></td>
			<td><%= @book.title %></td>
			<td><%= @book.body %></td>
		</tr>
		<% end %>
	</tbody>
</table>